<h1>Match results</h1>

<table>
  <thead>
    <tr>
      <td>
        Date
      </td>
      <td>
        Team 1
      </td>
      <td>
        Team 2
      </td>
      <td>
        Result
      </td>
    </tr>
  </thead>
  <tbody>
    {{#each results}}
        <tr>
          <td>
            {{ date }}
          </td>
          <td>
            {{#with team1}}
                {{ name }}
            {{/with}}
          </td>
          <td>
            {{#with team2}}
                {{ name }}
            {{/with}}
          </td>
          <td>
            {{ score1 }} - {{ score2 }}
          </td>
        </tr>

        {{#unless date}}
            {{test }}
        {{/unless}}
    {{/each}}
  </tbody>
</table>